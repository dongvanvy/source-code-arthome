    <script>
        function initMap() {
            var myLatLng = { lat: @Html.Raw(Json.Encode(usc.SupCheckIn_Latidue)), lng: @Html.Raw(Json.Encode(usc.SupCheckIn_Longtidue)) };
            var shoplatlng = { lat: @Html.Raw(Json.Encode(shop.Shop_latidue)), lng: @Html.Raw(Json.Encode(shop.Shop_Longtidue)) };

            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 20,
                center: myLatLng
            });

            var marker = new google.maps.Marker({
                position: myLatLng,
                map: map,
                title: @Html.Raw(Json.Encode(big.Us_id))
            });
            marker = new google.maps.Marker({
                position: shoplatlng,
                map: map,
                title: @Html.Raw(Json.Encode(shop.Shop_name))
            });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBg6tw7f4MnvH59vI2BTtilLJyxrbC1-eo&callback=initMap" async defer>
    </script>
